<?xml version="1.0"?>

<config>
    stopAtCycle=10000000000
    run-mode=both
</config>


<sst>
	<component id="genericProc" >	
		<genericProc>
			<params>
			  <clock>2.0 GHz</clock>
			  <execFile>sst/trunk/components/genericProc/programs/fooPrint</execFile>
			  <execFile>sst/trunk/components/genericProc/programs/gups_distro/gups_serial</execFile>
			  <execFile>sst/trunk/components/genericProc/programs/pthreadTest</execFile>
			  <execFile>sst/trunk/components/genericProc/programs/memBludge</execFile>
			  <execFile>sst/trunk/sst/elements/genericProc/programs/gups_distro/gups_notserial</execFile>
			  <externalMem>1</externalMem>
			  <maxMMOut>64</maxMMOut>
			  <verbose>5</verbose>
			  <ssBackEnd>1</ssBackEnd>
			  <cores>33</cores>
			  <ssConfig>sst/trunk/sst/elements/sampleConfigs/rs2.conf</ssConfig>
			  <mem.initialCredit>   8 </mem.initialCredit>
			  <mem.readRespCredit>  1 </mem.readRespCredit>
			  <mem.writeRespCredit> 1 </mem.writeRespCredit>
			 </params>
			<links>
				<link id="cpu2mem">
					<params>
						<name>mem0</name>
						<lat>1 ns</lat>
					</params>
				</link>
			</links>
		</genericProc>
	</component>

 	<component id="mem0">
 		<DRAMSimC>
 			<params>
			  <clock>1.0 Ghz</clock>
			  <systemini>system.ini</systemini>
			  <deviceini>ini/DDR3_micron_32M_8B_x8_sg15.ini</deviceini>
			  <pwd>DRAMSim</pwd>
			  <printstats>no</printstats>
 			</params>
 			<links>
 				<link id="cpu2mem">
 					<params>
 						<name>bus</name>
 						<lat>1 ns</lat>
 					</params>
 				</link>
 			</links>
 		</DRAMSimC>
 	</component>

</sst>
