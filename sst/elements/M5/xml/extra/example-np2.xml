<?xml version="1.0"?>

<sdl version="2.0"/>

<config>
    stopAtCycle=1000000000s
</config>

<variables>
    <lat> 10ns </lat>
</variables>

<sst>
    <component name=system type=m5C.M5 rank=0 >
        <params>
            <configFile>exampleM5-n1.xml</configFile>
            <debug> 0 </debug>
            <M5debug> none </M5debug>
            <info>yes</info>
            <registerExit>yes</registerExit>
        </params>
        <link name=L2Bus-L2Cache port=L2Bus-L2Cache latency=$lat />
    </component>

    <component name=system type=m5C.M5 rank=1 >
        <params>
            <configFile>exampleM5-n2.xml</configFile>
            <debug> 0 </debug>
            <M5debug> none </M5debug>
            <info>yes</info>
            <registerExit>no</registerExit>
        </params>
        <link name=L2Bus-L2Cache port=L2Bus-L2Cache latency=$lat />
    </component>
</sst>
