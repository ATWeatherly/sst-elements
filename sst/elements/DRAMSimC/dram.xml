<sdl version="2.0"/>
<config>
    stopAtCycle=1ms
</config>
<sst>
    <component name="trace" type=DRAMSimTrace.DRAMSimTrace >
        <params> 
            <clock>2.2GHz</clock>
            <tracefile>mase_gcc.trc</tracefile>
            <pwd>../DRAMSimTrace/</pwd>
        </params>
        <link name="link" port="bus" latency="1ns" />
    </component>

    <component name="dramsimx" type=DRAMSimC.DRAMSimC >
        <params>
            <clock>2.2Ghz</clock>
            <systemini>system.ini.example</systemini>
            <deviceini>ini/DDR3_micron_32M_8B_x8_sg15.ini</deviceini>
            <pwd>.</pwd>
            <printstats>no</printstats>
        </params>
        <link name="link" port="bus" latency="1ns" />
    </component>

</sst>
